{
  "name": "process-flow-manager",
  "version": "1.0.0",
  "description": "Collaborative web application for distributed business knowledge collection and automatic process flow management",
  "main": "server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run serve",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "setup:db": "cd server && npm run setup",
    "test": "cd server && npm test"
  },
  "keywords": [
    "process-management",
    "workflow",
    "collaboration",
    "vue",
    "node"
  ],
  "author": "nojaja",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5"
  }
}
